{
	"name": "ai-tool-retriever",
	"type": "module",
	"version": "1.0.3",
	"description": "tool retriever for the ai-sdk lib",
	"author": "gorango",
	"license": "MIT",
	"homepage": "https://github.com/gorango/ai-tool-retriever",
	"repository": {
		"type": "git",
		"url": "https://github.com/gorango/ai-tool-retriever.git"
	},
	"bugs": "https://github.com/gorango/ai-tool-retriever/issues",
	"keywords": [
		"ai",
		"tool-retriever",
		"semantic-search",
		"embeddings",
		"ai-sdk"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./providers/embedding/transformers": {
			"types": "./dist/providers/embedding/transformers.d.ts",
			"import": "./dist/providers/embedding/transformers.js"
		},
		"./providers/store/in-memory": {
			"types": "./dist/providers/store/in-memory.d.ts",
			"import": "./dist/providers/store/in-memory.js"
		},
		"./utils": {
			"types": "./dist/utils/index.d.ts",
			"import": "./dist/utils/index.js"
		}
	},
	"bin": {
		"download": "./dist/providers/embedding/download.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup && tsc --emitDeclarationOnly --declaration"
	},
	"peerDependencies": {
		"@xenova/transformers": ">=2.17.0 <3.0.0",
		"ai": ">=5.0.0 <6.0.0"
	},
	"peerDependenciesMeta": {
		"@xenova/transformers": {
			"optional": true
		}
	},
	"devDependencies": {
		"@ai-sdk/openai": "^2.0.52",
		"@types/node": "^24.7.2",
		"@vitest/coverage-v8": "3.2.4",
		"@xenova/transformers": "^2.17.2",
		"ai": "^5.0.70",
		"tsup": "^8.5.0",
		"typescript": "^5.9.3",
		"vitest": "^3.2.4",
		"zod": "^4.1.12"
	}
}
